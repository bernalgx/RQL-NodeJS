extends layout

block content
    script(src='/socket.io/socket.io.js')
    script(type='text/javascript').
        var socket = io.connect('http://node.softwaregya.tk:5555');
        socket.on('news', function (data) {
            console.log(data);
        });
        var s = window.setInterval(function(){
            socket.emit('up', new Date().getTime());
            clearInterval(s);
        }, 2000);
        